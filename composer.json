{
  "name": "hitchwiki",
  "description": "The Hitchhiker's Guide to Hitchhiking the World",
  "keywords": ["mediawiki", "wiki", "hitchhiking"],
  "homepage": "http://hitchwiki.org/",
  "type": "project",
  "authors": [
    {
    	"name": "Hitchwiki"
    }
  ],
  "license": "MIT",
  "support": {
    "issues": "https://github.com/Hitchwiki/hitchwiki/issues",
    "source": "https://github.com/Hitchwiki/hitchwiki/",
    "irc": "irc://irc.freenode.net/hitchhiking"
  },
  "repositories": [
    {
      "type": "package",
      "package": {
        "name": "wiki",
        "version": "1.28.0",
        "type": "project",
        "dist": {
          "url": "https://releases.wikimedia.org/mediawiki/1.28/mediawiki-1.28.0.tar.gz",
          "type": "tar"
        }
      }
    }
  ],
  "require": {
    "wiki": "*"
  },
  "config": {
    "vendor-dir": "public/",
    "process-timeout": 100000
  },
  "scripts": {
    "install_vagrant": "scripts/vagrant/install.sh",
    "install_local": "scripts/install.sh",
    "build": "scripts/build.sh",
    "clean": "scripts/clean.sh"
  }
}
